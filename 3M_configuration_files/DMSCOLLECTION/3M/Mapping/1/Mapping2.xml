                            <instance_generator name="uriFromID">
                                <arg name="identifier">FM.FIND_MANNER_ID</arg>
                                <arg name="name">FM.FIND_MANNER_NAME</arg>
                            </instance_generator>
                        </entity>
                    </target_node>
                </range>
            </link>
            <link>
                <path>
                    <source_relation>COIN.FIND_DATE</source_relation>
                    <target_relation>
                        <relationship>P30i_custody_transferred_through</relationship>
                        <entity variable="tc1">
                            <type>E10_Transfer_of_Custody</type>
                            <instance_generator name="uriEvents">
                                <arg name="identifier">COIN.ID</arg>
                                <arg name="label">"Transfer_of_Custody"</arg>
                            </instance_generator>
                        </entity>
                        <relationship>P4_has_time-span</relationship>
                        <entity variable="ts1">
                            <type>E52_Time-Span</type>
                        </entity>
                        <relationship>P81_ongoing_throughout</relationship>
                    </target_relation>
                </path>
                <range>
                    <source_node>COIN.FIND_DATE</source_node>
                    <target_node>
                        <entity>
                            <type>http://www.w3.org/2000/01/rdf-schema#Literal</type>
                            <instance_generator name="uriTimeSpan">
                                <arg name="identifier">COIN.FIND_DATE</arg>
                            </instance_generator>
                        </entity>
                    </target_node>
                </range>
            </link>
            <link>
                <path>
                    <source_relation>COIN.AUTHORITY_ID</source_relation>
