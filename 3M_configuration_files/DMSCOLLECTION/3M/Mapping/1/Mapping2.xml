                    </target_relation>
                </path>
                <range>
                    <source_node>AUTHORITY</source_node>
                    <target_node>
                        <entity>
                            <type>E4_Period</type>
                            <instance_generator name="uriFromID">
                                <arg name="identifier">AUTHORITY.AUTHORITY_ID</arg>
                                <arg name="name">AUTHORITY.AUTHORITY_NAME</arg>
                            </instance_generator>
                        </entity>
                    </target_node>
                </range>
            </link>
            <link>
                <path>
                    <source_relation>COIN.ISSUER_ID</source_relation>
                    <target_relation>
                        <relationship>P108i_was_produced_by</relationship>
                        <entity variable="p1">
                            <type>E12_Production</type>
                            <instance_generator name="uriEvents">
                                <arg name="identifier">COIN.ID</arg>
                                <arg name="label">"Production_Event"</arg>
                            </instance_generator>
                        </entity>
                        <relationship>P17_was_motivated_by</relationship>
                        <entity variable="a1">
                            <type>E7_Activity</type>
                            <instance_generator name="uriEvents">
                                <arg name="identifier">COIN.ID</arg>
                                <arg name="identifier">"Motivation_Event"</arg>
                            </instance_generator>
                            <additional>
                                <relationship>P2_has_type</relationship>
                                <entity>
                                    <type>E55_Type</type>
                                    <instance_info><constant>Issuing</constant></instance_info>
                                </entity>
                            </additional>
                        </entity>
                        <relationship>P14_carried_out_by</relationship>
                    </target_relation>
                </path>
                <range>
                    <source_node>ISSUER</source_node>
                    <target_node>
                        <entity>
                            <type>E39_Actor</type>
                            <instance_generator name="uriForActors">
                                <arg name="identifier">ISSUER.PR_ID</arg>
                                <arg name="name">ISSUER.PR_NAME</arg>
                            </instance_generator>
                        </entity>
                    </target_node>
                </range>
            </link>
